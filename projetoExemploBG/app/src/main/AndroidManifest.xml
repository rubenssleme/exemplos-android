<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetoexemplobg" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ControladorTelaNavigationDrawer"
            android:label="@string/title_activity_controlador_tela_navigation_drawer"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".ControladorTelaPrincipal"
            android:label="Tela Principal Exemplo 3bInfo" >
        </activity>
        <activity android:name=".ControladorTelaLogin" />
        <activity
            android:name=".ControladorTelaSplash"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="Tela Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>