<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agendadecontatos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".controller.TelaSplash"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.TelaLogin"
            android:label="Login"></activity>
        <activity
            android:name=".controller.ConsultaContato"
            android:label="Consulta Contato" />
        <activity
            android:name=".controller.CadastrarContato"
            android:label="Cadastro Contato" />
        <activity android:name=".controller.MainActivity" android:label="Menu" />
    </application>

</manifest>