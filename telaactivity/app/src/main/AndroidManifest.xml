<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aprendaefaca.telaactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ControladorTelaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ControladorTelaActivity2"
            android:label="@string/titulo_tela2" />
        <activity
            android:name=".ControladorTelaScrollView"
            android:label="@string/title_activity_controlador_tela_scroll_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ControladorTelaBottomNavigation"
            android:label="@string/title_activity_controlador_tela_bottom_navigation"></activity>
    </application>

</manifest>